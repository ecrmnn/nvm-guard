{
  "name": "nvm-guard",
  "version": "1.0.1",
  "description": "Enforce correct .nvmrc version when running npm scripts",
  "bin": {
    "nvm-guard": "dist/index.js"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register test/tests.ts && npm run eslint",
    "eslint": "node_modules/.bin/eslint src/ --fix --ext .ts",
    "build": "rm -rf dist && tsc",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ecrmnn/nvm-guard"
  },
  "author": {
    "name": "Daniel Eckermann",
    "email": "danieleckermann@gmail.com",
    "url": "http://danieleckermann.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecrmnn/nvm-guard/issues"
  },
  "homepage": "https://github.com/ecrmnn/nvm-guard",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.6.2",
    "@typescript-eslint/parser": "^1.3.0",
    "chai": "^4.1.2",
    "eslint": "^5.14.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^5.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "compatible-version": "^1.0.0"
  }
}
